{
  "name": "3d-warehouse-visualization",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:auto": "node scripts/next-port.mjs",
    "dev:logs": "node tools/log-server.mjs",
    "build": "vite build",
    "start": "serve dist -l tcp://0.0.0.0:${PORT:-8080} -s",
    "gcp-build": "npm run build",
    "lint": "eslint .",
    "preview": "vite preview",
    "agent": "node scripts/start-agent.js",
    "dev:port": "vite --port",
    "clean:dry": "node scripts/clean-project.mjs --dry",
    "clean:apply": "node scripts/clean-project.mjs --apply",
    "mobile:preflight": "tsc --noEmit && npm run build && node -e \"const fs=require('fs'); const stat=fs.statSync('dist/assets/index*.js'); console.log('Bundle size:', (stat.size/1024).toFixed(0), 'KB');\"",
    "test:playwright:mobile": "playwright test tests/playwright.mobile.spec.ts",
    "audit:mobile": "npm run build && npm run start & sleep 5 && PREVIEW_URL=http://localhost:8080 npm run test:playwright:mobile && npx lhci autorun && pkill -f 'serve dist'",
    "test:ui": "playwright test",
    "audit:ui": "npm run build && npm run test:ui",
    "agent:mobile": "node agent/run-mobile-agent.mjs",
    "agent:mobile:quick": "node agent/run-mobile-agent.mjs --quick",
    "agent:mobile:dry": "node agent/run-mobile-agent.mjs --dry-run"
  },
  "dependencies": {
    "@react-three/drei": "^9.99.7",
    "@react-three/fiber": "^8.0.0",
    "@react-three/postprocessing": "^2.19.1",
    "clsx": "^2.1.1",
    "framer-motion": "^10.18.0",
    "hls.js": "^1.6.14",
    "lucide-react": "^0.344.0",
    "mitt": "^3.0.1",
    "papaparse": "^5.5.3",
    "postprocessing": "^6.37.8",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "serve": "^14.2.0",
    "tailwind-merge": "^3.3.1",
    "three": "^0.162.0",
    "three-gpu-pathtracer": "^0.0.23",
    "three-stdlib": "^2.36.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@gltf-transform/cli": "^4.2.1",
    "@gltf-transform/core": "^4.2.1",
    "@gltf-transform/extensions": "^4.2.1",
    "@lhci/cli": "^0.15.1",
    "@playwright/test": "^1.56.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/three": "^0.162.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "gltf-pipeline": "^4.3.0",
    "js-yaml": "^4.1.0",
    "jsdom": "^27.1.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "vite": "^5.4.21"
  }
}
